{
	"_version": "1.7.0",
	"sap.app": {
		"id": "com.sapZSQRMBWA",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZSQRMBWA": {
				"uri": "/sap/opu/odata/sap/ZSQRMBWA_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZSQRMBWA_SRV/metadata.xml",
					"annotations": [
						"annotation"
					]
				}
			},
			"annotation": {
				"type": "ODataAnnotation",
				"uri": "localService/annotation.xml",
				"settings": {
					"localUri": "localService/annotation.xml"
				}
			}
		}
	},
	"sap.ui": {
		"fullWidth": true,
		"technology": "UI5",
		"sapFiori2Adaptation": true,
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"handleValidation": true,
		"config": {
			"sapFiori2Adaptation": true,
			"fullWidth": true
		},
		"rootView": {
			"viewName": "com.sapZSQRMBWA.view.ListView",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {},
				"sap.ui.table": {},
				"sap.ui.unified": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sapZSQRMBWA.i18n.i18n"
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"defaultUpdateMethod": "PUT",
					"preliminaryContext": true
				},
				"dataSource": "ZSQRMBWA",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.sapZSQRMBWA.view",
				"controlId": "firstView",
				"controlAggregation": "pages",
				"clearAggregation": false,
				"viewLevel": 1
			},
			"routes": [
				{
					"name": "ListView",
					"pattern": "",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"ListView"
					]
				},
				{
					"pattern": "ListView/:context:",
					"name": "InspectionView",
					"target": [
						"InspectionView"
					]
				},
				{
					"pattern": "Mainboard/:context:",
					"name": "AddInspection",
					"target": [
						"AddInspection"
					]
				}
			],
			"targets": {
				"InspectionView": {
					"viewName": "EditInspection",
					"viewId": "newinspection",
					"controlAggregation": "pages",
					"transition": "slide",
					"viewLevel": 2
				},
				"ListView": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ListView",
					"viewId": "firstView",
					"viewLevel": 1
				},
				"AddInspection": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewId": "addinspection",
					"viewName": "CreateInspection",
					"viewLevel": 1
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zsqrmbwa/webapp",
		"_version": "1.1.0"
	}
}