<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="{i18n>noSupplierFound}"
		title="{i18n>chooseSupplier}"
		search="handleSupplierSearch"
		confirm="handleSupplierClose"
		cancel="handleSupplierCloseNavigate"
		liveChange="HandleLiveSupplierSearch"
		items="{
			path: '/ZWM_SUPPLIER_VH'
		}" >
		<StandardListItem
			title="{name1}"
			description="{lifnr}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>